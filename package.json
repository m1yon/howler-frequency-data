{
  "version": "0.1.4",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/m1yon/howler-frequency-data"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "howler-frequency-data",
  "description": "Get byte frequency data from any amount of Howler.js objects using the Web Audio API.",
  "author": "Michael Lyon",
  "module": "dist/howler-frequency-data.esm.js",
  "size-limit": [
    {
      "path": "dist/howler-frequency-data.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/howler-frequency-data.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.1",
    "@types/howler": "^2.2.2",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "husky": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^5.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "howler": "^2.2.3",
    "react-use": "^17.2.4"
  }
}
